  "description": "Extension modules for ESTA WebJS",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build esta-webjs-extensions --prod",
    "postbuild": "copyfiles -u 3 projects/esta-webjs-extensions/src/styles/*.scss dist/esta-webjs-extensions",
    "test": "ng test esta-webjs-extensions --code-coverage --watch=false --browsers ChromeHeadless --progress=false",
